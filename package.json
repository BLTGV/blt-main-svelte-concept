{
  "name": "blt-main-svelte-concept",
  "description": "Working Concepts for Svelte",
  "repository": {
    "url": "https://github.com/BLTGV/blt-main-svelte-concept"
  },
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build": "sapper build --legacy",
    "export": "sapper export --basepath \"blt-main-svelte-concept\" && rm -rf docs/* && cp -rf __sapper__/export/blt-main-svelte-concept/* docs",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.49.0",
    "tailwindcss": "^1.1.2"
  }
}
